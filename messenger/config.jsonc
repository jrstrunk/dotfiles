{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "size": {
      "binaryPrefix": "jedec"
    }
  },
  "modules": [
    "title",
    "separator",
    "kernel",
    "shell",
    "board",
    {
      "type": "command",
      "text": "echo 'Edge'",
      "key": "Chassis"
    },
    {
      "type": "memory",
      "format": "{2} ({3})"
    },
    {
      "type": "swap",
      "format": "{2} ({3})"
    },
    {
      "type": "cpu",
      "format": "Pentium N3700 x4 (1250)"
    },
    {
      "type": "gpu",
      "key": "GPU (Integrated)",
      "format": "Intel Device 22B1"
    },
    {
      "type": "disk",
      "format": "{2} - {9} ({3})"
    },
    "uptime",
    "datetime",
    {
      "type": "localip",
      "key": "Local IP"
    },
    "break",
    "colors"
  ]
}
